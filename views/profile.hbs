      <main>
            {{#each profilerow}}
            <div class="container1">
                  <div class="boxa" style="margin-top: -10vh;">
                        <h2>Profile</h2>
                        
                        <form action="/dashboard/update/user" method="POST" >
                        
                              <div class="form-group">
                                    <label for="Name">Name</label>
                                    
                                    <input type="text" value={{this.name}} class="form-control" id="Name" placeholder="Name" name="name">
                              </div>
                              <div class="form-group" class="dabba">
                                    <label for="Email">Email</label>
                               
                                    <input type="email" class="form-control" id="Email"
                                       value="{{this.email}}"   placeholder="youremail@gmail.com" name="email">
                             
                              </div>
                              <div class="form-group">
                                    <label for="Phone">Phone Number</label>
                             
                                    <input type="text" value="{{this.phone_number}}" class="form-control" id="Phone" placeholder="Phone Number" name="phone_number">
                            
                              </div>
                              <div class="form-group">
                                    <label for="sem">Semester</label>
                            
                                    <input type="text" value="{{this.semester}}" class="form-control" id="sem" placeholder="Semester" name="semester">
                                
                              </div>
                              <div class="form-group">
                                    <label for="Enrollment Number">Enrollment Number</label>
                                  
                                    <input type="text" class="form-control" id="Enrollment Number"
                                       value="{{this.enrollment_number}}"   placeholder="Enrollment Number" name="enrollment_number">
                                     
                              </div>
                              <div class="form-group">
                                    <label for="Roll Number">Roll Number</label>
                                    
                                    <input type="text" value="{{this.roll_number}}" class="form-control" id="Roll Number" placeholder="Roll Number" name="roll_number">
                                    
                              </div>
                                                     
                        <button class="btnmain1" type="submit" >UPDATE </button></span><span class="dem"></span>
                        </form>
                  </div>

                  <div class="img1">
                        <img src="img/img.jpg" alt="">
                  </div>
            </div>
            {{/each}}
            
